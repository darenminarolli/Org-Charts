{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "charts-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/charts/ChartsWebPart.js",
          "manifest": "./src/webparts/charts/ChartsWebPart.manifest.json"
        }
      ]
    },
    "summary-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/summary/SummaryWebPart.js",
          "manifest": "./src/webparts/summary/SummaryWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ChartsWebPartStrings": "lib/webparts/charts/loc/{locale}.js",
    "SummaryWebPartStrings": "lib/webparts/summary/loc/{locale}.js"
  }
}
